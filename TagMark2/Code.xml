function RetTag(aName, valPairs = null)
{
  res = {'Tag':aName, 'vals':[]};
  if (null != valPairs )
  for(var n = 0; n < valPairs.length; n++ )
  {
    t = valPairs[n];
    for(var k in t)
      res.vals.push({'nm':k , 'v':t[k ]});
  }
  return res;
}

